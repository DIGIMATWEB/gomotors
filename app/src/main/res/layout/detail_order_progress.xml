<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="120dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="1dp"
        android:background="@drawable/shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <View
        android:id="@+id/view11"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/viewrecolectar"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/shape_circle_process"
        app:layout_constraintBottom_toBottomOf="@+id/view11"
        app:layout_constraintStart_toStartOf="@+id/view11"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recolectar"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/viewrecolectar"
        app:layout_constraintStart_toStartOf="@+id/view11"
        app:layout_constraintTop_toBottomOf="@+id/viewrecolectar" />

    <View
        android:id="@+id/viewEncola"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/shape_circle_process_not"
        app:layout_constraintBottom_toBottomOf="@+id/view11"
        app:layout_constraintEnd_toStartOf="@+id/guidelinesteps2"
        app:layout_constraintStart_toStartOf="@+id/guidelinesteps2"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Estado "
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recolectado"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/viewEncola"
        app:layout_constraintStart_toStartOf="@+id/viewEncola"
        app:layout_constraintTop_toBottomOf="@+id/viewEncola" />

    <View
        android:id="@+id/viewenprogreso"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/shape_circle_process_not"
        app:layout_constraintBottom_toBottomOf="@+id/view11"
        app:layout_constraintEnd_toStartOf="@+id/guidelinesteps3"
        app:layout_constraintStart_toStartOf="@+id/guidelinesteps3"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="En camino"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/viewenprogreso"
        app:layout_constraintStart_toStartOf="@+id/viewenprogreso"
        app:layout_constraintTop_toBottomOf="@+id/viewEncola" />

    <View
        android:id="@+id/viewterminado"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/shape_circle_process_not"
        app:layout_constraintBottom_toBottomOf="@+id/view11"
        app:layout_constraintEnd_toEndOf="@+id/view11"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Terminado"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/view11"
        app:layout_constraintStart_toStartOf="@+id/viewterminado"
        app:layout_constraintTop_toBottomOf="@+id/viewEncola" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelinesteps1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelinesteps2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.36" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelinesteps3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.64" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelinesteps4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />
</androidx.constraintlayout.widget.ConstraintLayout>